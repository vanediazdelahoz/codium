{
  "name": "codium",
  "version": "1.0.0",
  "private": true,
  "description": "Plataforma para evaluar algoritmos - Juez Online",
  "license": "MIT",
  "author": "Codium Team",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@infrastructure/(.*)$": "<rootDir>/infrastructure/$1",
      "^@interface/(.*)$": "<rootDir>/interface/$1"
    },
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@application/auth": "latest",
    "@application/challenges": "latest",
    "@application/courses": "latest",
    "@application/submissions": "latest",
    "@domain/entities": "latest",
    "@domain/repositories": "latest",
    "@fastify/static": "latest",
    "@google-cloud/spanner": "latest",
    "@nestjs/bull": "latest",
    "@nestjs/common": "latest",
    "@nestjs/config": "latest",
    "@nestjs/core": "latest",
    "@nestjs/jwt": "latest",
    "@nestjs/microservices": "latest",
    "@nestjs/passport": "latest",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "latest",
    "@nestjs/typeorm": "latest",
    "@nestjs/websockets": "latest",
    "@prisma/client": "latest",
    "@sap/hana-client": "latest",
    "bcrypt": "^5.1.1",
    "bcryptjs": "latest",
    "better-sqlite3": "latest",
    "bull": "latest",
    "class-transformer": "^0.5.1",
    "class-validator": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "latest",
    "lucide-react": "^0.454.0",
    "mongodb": "latest",
    "mssql": "latest",
    "mysql2": "latest",
    "next": "15.2.4",
    "oracledb": "latest",
    "passport": "^0.7.0",
    "passport-jwt": "latest",
    "passport-local": "^1.0.0",
    "pg": "latest",
    "pg-native": "latest",
    "pg-query-stream": "latest",
    "react": "^19",
    "react-dom": "^19",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sql.js": "latest",
    "sqlite3": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "typeorm": "latest",
    "typeorm-aurora-data-api-driver": "latest",
    "uuid": "latest"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
